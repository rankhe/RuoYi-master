<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.product.mapper.QuotationBlackUserMapper">
    
    <resultMap type="QuotationBlackUser" id="QuotationBlackUserResult">
        <result property="id"    column="id"    />
        <result property="userId"    column="user_id"    />
        <result property="mobile"    column="mobile"    />
        <result property="ownerUserId"    column="owner_user_id"    />
        <result property="updateBy"    column="update_by"    />
        <result property="status"    column="status"    />
        <result property="updateTime"    column="update_time"    />
        <result property="createTime"    column="create_time"    />
    </resultMap>

    <sql id="selectQuotationBlackUserVo">
        select id, user_id, mobile, owner_user_id, update_by, status, update_time, create_time from quotation_black_user
    </sql>

    <select id="selectQuotationBlackUserList" parameterType="QuotationBlackUser" resultMap="QuotationBlackUserResult">
        <include refid="selectQuotationBlackUserVo"/>
        <where>  
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="mobile != null  and mobile != ''"> and mobile = #{mobile}</if>
            <if test="ownerUserId != null "> and owner_user_id = #{ownerUserId}</if>
            <if test="status != null "> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectQuotationBlackUserById" parameterType="Long" resultMap="QuotationBlackUserResult">
        <include refid="selectQuotationBlackUserVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertQuotationBlackUser" parameterType="QuotationBlackUser" useGeneratedKeys="true" keyProperty="id">
        insert into quotation_black_user
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">user_id,</if>
            <if test="mobile != null and mobile != ''">mobile,</if>
            <if test="ownerUserId != null">owner_user_id,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="status != null">status,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="mobile != null and mobile != ''">#{mobile},</if>
            <if test="ownerUserId != null">#{ownerUserId},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="status != null">#{status},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateQuotationBlackUser" parameterType="QuotationBlackUser">
        update quotation_black_user
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="mobile != null and mobile != ''">mobile = #{mobile},</if>
            <if test="ownerUserId != null">owner_user_id = #{ownerUserId},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="status != null">status = #{status},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteQuotationBlackUserById" parameterType="Long">
        delete from quotation_black_user where id = #{id}
    </delete>

    <delete id="deleteQuotationBlackUserByIds" parameterType="String">
        delete from quotation_black_user where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>